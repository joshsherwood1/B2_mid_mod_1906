<h2>All Books</h2>
<p><% @books.each do |book| %></p>
<section id="book-<%=book.id%>">
  <p>Title: <%= book.title %></p>
  <p>Number of Pages: <%= book.number_of_pages %></p>
  <p>Publication Year: <%= book.publication_year %></p>
  <p>Author(s): <% book.authors.each do |author| %></p>
    <%=link_to(author.name, "/authors/#{author.id}")%>
    <% end %>
    </section>
<% end %>
